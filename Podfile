use_frameworks!

def testing_pods
  
  pod 'SDWebImage'
end


target 'SwiftRadio' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    # Pods for SwiftRadio
    testing_pods
    
    pod 'AppCenter'
    pod 'AppCenter/Push'
    
  end

  target 'SwiftRadioUITests' do
    inherit! :search_paths
    # Pods for testing
  end
  
  target 'SwiftRadioTV' do
      testing_pods
   end

